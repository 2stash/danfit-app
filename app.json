{
  "expo": {
    "name": "danfit",
    "slug": "danfit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "come.DanFit.wfhwod",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "come.DanFit.wfhwod",
      "versionCode": 1,
      "permissions":["NOTIFICATIONS", "USER_FACING_NOTIFICATIONS"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
